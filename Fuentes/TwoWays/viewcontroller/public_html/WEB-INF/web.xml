<?xml version = '1.0' encoding = 'windows-1252'?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
    <description>TwoWays Application</description>
    <filter>
        <filter-name>adfFaces</filter-name>
        <filter-class>oracle.adf.view.faces.webapp.AdfFacesFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>adfFaces</filter-name>
        <servlet-name>*.jsp</servlet-name>
    </filter-mapping>
    <filter-mapping>
        <filter-name>adfFaces</filter-name>
        <url-pattern>*.jspx</url-pattern>
    </filter-mapping>
    <servlet>
        <servlet-name>Log4jSetupServlet</servlet-name>
        <servlet-class>com.twoways.util.Log4jSetupServlet</servlet-class>
        <init-param>
            <param-name>props</param-name>
            <param-value>/WEB-INF/log4j.xml</param-value>
        </init-param>
        <init-param>
            <param-name>watch</param-name>
            <param-value>false</param-value>
        </init-param>
        <init-param>
            <param-name>debug_mode</param-name>
            <param-value>true</param-value>
        </init-param>
        <load-on-startup>1</load-on-startup>
    </servlet>
 <servlet>
  <servlet-name>dwr-invoker</servlet-name>
  <display-name>DWR Servlet</display-name>
  <servlet-class>org.directwebremoting.servlet.DwrServlet</servlet-class>
  <init-param>
     <param-name>debug</param-name>
     <param-value>true</param-value>
  </init-param>
  <init-param>
      <param-name>crossDomainSessionSecurity</param-name>
      <param-value>false</param-value>
 </init-param>
</servlet>
    <servlet>
        <servlet-name>AbmClientesServlet</servlet-name>
        <servlet-class>com.twoways.view.servlets.AbmClientesServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AbmUsuariosServlet</servlet-name>
        <servlet-class>com.twoways.view.servlets.AbmUsuariosServlet</servlet-class>
    </servlet>
     <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>com.twoways.view.servlets.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AbmEmpleadosServlet</servlet-name>
        <servlet-class>com.twoways.view.servlets.AbmEmpleadosServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AbmItemsServlet</servlet-name>
        <servlet-class>com.twoways.view.servlets.AbmItemsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>OrdenTrabajoServlet</servlet-name>
        <servlet-class>com.twoways.view.servlets.OrdenTrabajoServlet</servlet-class>
    </servlet>
     <servlet>
        <servlet-name>DownloadFileServlet</servlet-name>
        <servlet-class>com.twoways.view.servlets.DownloadFileServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AbmCuentasServlet</servlet-name>
        <servlet-class>com.twoways.view.servlets.AbmCuentasServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BuscarOrdenTrabajoServlet</servlet-name>
        <servlet-class>com.twoways.view.servlets.BuscarOrdenTrabajoServlet</servlet-class>
    </servlet>
      <servlet>
        <servlet-name>GestionarProyectoServlet</servlet-name>
        <servlet-class>com.twoways.view.servlets.GestionarProyectoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProyectoServlet</servlet-name>
        <servlet-class>com.twoways.view.servlets.ProyectosServlet</servlet-class>
    </servlet>
     <servlet>
        <servlet-name>AsignacionesServlet</servlet-name>
        <servlet-class>com.twoways.view.servlets.AsignacionesServlet</servlet-class>
    </servlet>
      <servlet>
        <servlet-name>AbmTarifasServlet</servlet-name>
        <servlet-class>com.twoways.view.servlets.AbmTarifasServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AbmGastosServlet</servlet-name>
        <servlet-class>com.twoways.view.servlets.AbmGastosServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListaEmpleadosServlet</servlet-name>
        <servlet-class>com.twoways.view.servlets.ListaEmpleadosServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AbmPagosServlet</servlet-name>
        <servlet-class>com.twoways.view.servlets.AbmPagosServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AbmCotizacionesServlet</servlet-name>
        <servlet-class>com.twoways.view.servlets.AbmCotizacionesServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AbmFacturacionServlet</servlet-name>
        <servlet-class>com.twoways.view.servlets.AbmFacturacionServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListaCobrosServlet</servlet-name>
        <servlet-class>com.twoways.view.servlets.ListaCobrosServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>resources</servlet-name>
        <servlet-class>oracle.adf.view.faces.webapp.ResourceServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AbmClientesServlet</servlet-name>
        <url-pattern>/clientes</url-pattern>
    </servlet-mapping>
  
    <servlet-mapping>
        <servlet-name>AbmTarifasServlet</servlet-name>
        <url-pattern>/tarifas</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AbmUsuariosServlet</servlet-name>
        <url-pattern>/usuarios</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AbmEmpleadosServlet</servlet-name>
        <url-pattern>/empleados</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AbmItemsServlet</servlet-name>
        <url-pattern>/items</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OrdenTrabajoServlet</servlet-name>
        <url-pattern>/ordentrabajo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DownloadFileServlet</servlet-name>
        <url-pattern>/downloadfile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AbmCuentasServlet</servlet-name>
        <url-pattern>/cuentas</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BuscarOrdenTrabajoServlet</servlet-name>
        <url-pattern>/buscarordentrabajo</url-pattern>
    </servlet-mapping>
     <servlet-mapping>
        <servlet-name>GestionarProyectoServlet</servlet-name>
        <url-pattern>/gestionarproyecto</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProyectoServlet</servlet-name>
        <url-pattern>/proyectos</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AsignacionesServlet</servlet-name>
        <url-pattern>/asignaciones</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AbmGastosServlet</servlet-name>
        <url-pattern>/gastos</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
    <servlet-name>dwr-invoker</servlet-name>
     <url-pattern>/dwr/*</url-pattern>
   </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListaEmpleadosServlet</servlet-name>
        <url-pattern>/listaEmpleados</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AbmPagosServlet</servlet-name>
        <url-pattern>/pagos</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AbmCotizacionesServlet</servlet-name>
        <url-pattern>/cotizaciones</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AbmFacturacionServlet</servlet-name>
        <url-pattern>/facturacion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListaCobrosServlet</servlet-name>
        <url-pattern>/listaFacturas</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>resources</servlet-name>
        <url-pattern>/adf/*</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>-1</session-timeout>
    </session-config>
    <mime-mapping>
        <extension>html</extension>
        <mime-type>text/html</mime-type>
    </mime-mapping>
    <mime-mapping>
        <extension>txt</extension>
        <mime-type>text/plain</mime-type>
    </mime-mapping>
    <welcome-file-list>
       <welcome-file>/login.jsp</welcome-file>
    </welcome-file-list>
</web-app>
